{
  "name": "iBot Premium",
  "description": "Premium version of iBot",
  "repository": "https://github.com/aaronnsmh/ibotv1-premium",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["bot", "heroku", "music-playback", "discord", "javascript", "node", "youtube"],
  "env": {
    "BOT_TOKEN": {
      "description": "Discord token used to log in",
      "required": true
    },
    "group": {
      "description": "Roblox group",
      "required": false
    },
    "rank": {
      "description": "Maxrank the bot can rank to in the group",
      "required": false
    },
    "username": {
      "description": "Roblox account usernmae",
      "required": false
    },
    "password": {
      "description": "Roblox account password",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs"
    },
    {
      "url": "https://github.com/issueapp/heroku-buildpack-ffmpeg"
    }
  ]
}
